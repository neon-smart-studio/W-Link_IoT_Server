    function Get_Source_Device_Select_Node_Index(index_str)
    {
        var src_dev_node_index = null;
        var selected_btn_id = $('#device_'+index_str+'_switch_list .js-active').attr("id");
        if(selected_btn_id==null){
            return null;
        }
        var btn_header_str = "device_"+index_str+"_select_switch_";
        var btn_header_length = btn_header_str.length;
        var btn_header_start_idx = selected_btn_id.indexOf(btn_header_str);
        if(btn_header_start_idx>=0)
        {
            src_dev_node_index = Number(selected_btn_id.substring(btn_header_length));
        }
        return src_dev_node_index;
    }

    function Get_Source_Device_Select_Action(index_str)
    {
        var src_dev_action = null;
        var selected_tile_id = $('#device_'+index_str+'_support_actions_list .selected').attr("id");
        if(selected_tile_id==null){
            return null;
        }
        var tile_header_str = "device_"+index_str+"_switch_action_";
        var tile_header_length = tile_header_str.length;
        var tile_header_start_idx = selected_tile_id.indexOf(tile_header_str);
        if(tile_header_start_idx>=0)
        {
            src_dev_action = selected_tile_id.substring(tile_header_length);
        }
        return src_dev_action;
    }

    function Update_Bind_Action_Settings(index_str)
    {
        var src_dev_node_index = Get_Source_Device_Select_Node_Index(index_str)
        if(src_dev_node_index==null){
            return null;
        }

        var src_dev_action = Get_Source_Device_Select_Action(index_str)
        if(src_dev_action==null){
            return null;
        }

        var device_type = Get_Accessories_Device_Type_By_Index(Number(index_str));
        var device_id = Get_Accessories_Device_ID_By_Index(Number(index_str));
        
        GET_Bind_Action_Info(device_type, device_id, src_dev_node_index, src_dev_action, function(rsp_json){
            Metro.getPlugin('#select_bind_target_address_type_'+index_str, 'select').val([rsp_json.target_address_type]);
            
            <% if(bind_unbind_multi_target_topic_type){ %>
                if(Do_Check_Target_Topic_Match(rsp_json.command_type))
                {
                    Metro.getPlugin('#select_bind_target_topic_type_'+index_str, 'select').val([rsp_json.command_type]);
                }
            <% }  %>
        
            $('#bind_unbind_selected_target_text_area_'+index_str).val(rsp_json.target_address_ID);

            Handle_Target_Address_ID_Changed(rsp_json.target_address_type, rsp_json.command_type, index_str, rsp_json.target_address_ID);
            
            if(rsp_json.command!="default" && rsp_json.command!="Default")
            {
                if(Do_Check_Target_Topic_Match(rsp_json.command_type))
                {
                    Show_Bind_Unbind_Target_Action_Settings(rsp_json.target_address_type, rsp_json.command_type, index_str, "device_"+index_str+"_target_action_settings_display_area");

                    Handle_Update_Bind_Action_Settings_Info(rsp_json.target_address_type,
                                                            rsp_json.command_type, 
                                                            rsp_json.target_address_ID, 
                                                            index_str, rsp_json);
                }
            }
        });
    }
    
