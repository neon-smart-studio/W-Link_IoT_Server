    function onClick_Show_Target_List_Btn(index_str)
    {
        var target_address_type = Metro.getPlugin('#select_bind_target_address_type_'+index_str, 'select').val();
        if(target_address_type==null){return;}
         
        var target_topic_type = "default";

        <% 
        if(service_page_info!=null && current_topic_page_info!=null && current_sub_service_page_info!=null){
            var bind_unbind_multi_target_topic_type = false;
            if(current_sub_service_page_info.bind_unbind_multi_target_topic_type!=null &&
                current_sub_service_page_info.bind_unbind_target_topic_type_list!=null &&
                current_sub_service_page_info.bind_unbind_target_topic_icon_type_list!=null &&
                current_sub_service_page_info.bind_unbind_target_topic_icon_list!=null){
                    if(current_sub_service_page_info.bind_unbind_multi_target_topic_type=true){
                        bind_unbind_multi_target_topic_type = true;
                    }
            }
            if(bind_unbind_multi_target_topic_type){ %>
                target_topic_type = Metro.getPlugin('#select_bind_target_topic_type_'+index_str, 'select').val();
            <% } else { %>
                target_topic_type = "<%=current_sub_service_page_info.bind_unbind_target_topic_type%>";
            <% }
        } 
        %>

        if(target_address_type=="Broadcast")
        {

        }
        else{
            Metro.dialog.open('#select_bind_target_modal_' + index_str);
            if(target_address_type=="Device")
            {
                Print_Bind_Unbind_Avaliable_Device_List(index_str, target_topic_type, 'bind_unbind_target_list_' + index_str);
            }
            if(target_address_type=="Group")
            {
                Print_Bind_Unbind_Avaliable_Group_List(index_str, target_topic_type, 'bind_unbind_target_list_' + index_str);
            }
        }
    }

    function onClick_Selected_Target_Tile(index_str, display_area_id, address_ID)
    {
        var tile_selected = $('#device_'+index_str+'_bind_target_'+address_ID).hasClass("selected");

        $('#'+display_area_id+" div").removeClass("selected");

        if(!tile_selected)
        {
            $('#device_'+index_str+'_bind_target_'+address_ID).addClass("selected");
            Handle_Bind_Unbind_Target_Tile_Selected_Event(address_ID);
        }
        else
        {
            Handle_Bind_Unbind_Target_Tile_UnSelected_Event(address_ID);
        }

        if($('#'+display_area_id+" div").hasClass("selected"))
        {
            $('#bind_confirm_select_target_btn_'+index_str).prop('disabled', false);
        }
        else{
            $('#bind_confirm_select_target_btn_'+index_str).prop('disabled', true);
        }
    }

    function onClick_Confirm_Select_Target_Btn(index_str, display_area_id)
    {
        var selected_tile_id = $('#'+display_area_id+" .selected").attr("id");
        if(selected_tile_id==null){return;}
        var header_str = "device_"+index_str+"_bind_target_";
        var header_length = header_str.length;
        var header_start_idx = selected_tile_id.indexOf(header_str);
        if(header_start_idx>=0)
        {
            var address_ID = selected_tile_id.substring(header_length);

            <% if(bind_unbind_multi_target_topic_type){ %>
                var target_topic_type = Metro.getPlugin('#select_bind_target_topic_type_'+index_str, 'select').val();
            <% } 
            else { %>
                var target_topic_type = "<%=current_sub_service_page_info.bind_unbind_target_topic_type%>";
            <% }  %>
        
            Handle_Target_Address_ID_Changed(Metro.getPlugin('#select_bind_target_address_type_'+index_str, 'select').val(), target_topic_type, index_str, address_ID);
            $('#bind_unbind_selected_target_text_area_'+index_str).val(address_ID);
            Metro.dialog.close('#select_bind_target_modal_' + index_str);
        }
    }

    function onClick_Save_Bind_Settings_Btn(index_str, display_area_id)
    {
        var src_dev_node_index = null;
        var selected_btn_id = $('#device_'+index_str+'_switch_list .js-active').attr("id");
        if(selected_btn_id==null){
            Print_Status_Error("device_"+index_str+"_select_switch_status");
            return;
        }
        var btn_header_str = "device_"+index_str+"_select_switch_";
        var btn_header_length = btn_header_str.length;
        var btn_header_start_idx = selected_btn_id.indexOf(btn_header_str);
        if(btn_header_start_idx>=0)
        {
            src_dev_node_index = Number(selected_btn_id.substring(btn_header_length));
        }
        if(src_dev_node_index==null)
        {
            Print_Status_Error("device_"+index_str+"_select_switch_status");
            return;
        }
        Print_Status_OK("device_"+index_str+"_select_switch_status");

        var src_dev_action = null;
        var selected_tile_id = $('#device_'+index_str+'_support_actions_list .selected').attr("id");
        if(selected_tile_id==null){
            Print_Status_Error("device_"+index_str+"_select_action_status");
            return;
        }
        var tile_header_str = "device_"+index_str+"_switch_action_";
        var tile_header_length = tile_header_str.length;
        var tile_header_start_idx = selected_tile_id.indexOf(tile_header_str);
        if(tile_header_start_idx>=0)
        {
            src_dev_action = selected_tile_id.substring(tile_header_length);
        }
        if(src_dev_action==null)
        {
            Print_Status_Error("device_"+index_str+"_select_action_status");
            return;
        }
        Print_Status_OK("device_"+index_str+"_select_action_status");

        var target_address_type = Metro.getPlugin('#select_bind_target_address_type_'+index_str, 'select').val();
