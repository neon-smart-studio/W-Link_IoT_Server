                        show_support_actions_bar.push(Resolv_Accessories_Device_Action_Icon(device_type, support_actions[i]));
                        show_support_actions_bar.push("</div>");
                    }
                    show_support_actions_bar.push("</div>");

                    $('#device_'+index_str+"_support_actions_display_area").html('');
                    $('#device_'+index_str+"_support_actions_display_area").html(show_support_actions_bar.join(''));
                }
            });
        });
    }

    function onClick_GoBack_Accessories_List_Btn(index_str)
    {
        var need_save = false;
        if(need_save_change[Number(index_str)]!=null)
        {
            if(need_save_change[Number(index_str)]==true)
            {
                need_save = true;
            }
        }

        if(need_save)
        {
            Metro.dialog.open('#confirm_goback_without_saveing_changes_modal_' + index_str);
        }
        else{
            Show_Accessories_List_Page();
        }
    }

    </script> 
    <% } %>
</head>
