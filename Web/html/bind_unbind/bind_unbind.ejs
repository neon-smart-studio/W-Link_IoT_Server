    function Bind_Unbind_Update_Accessories_Device_List()
    {
        GET_All_Accessories_Device_List(function(rsp_json){
            map_accessories_info_list = [];

            var show_accessories_html = [];
            
            for (var i = 0; i < rsp_json.device_list.length; i++) {
                map_accessories_info_list.push({
                    "device_ID":rsp_json.device_list[i].device_ID,
                    "device_Name":rsp_json.device_list[i].device_Name,
                    "device_Type":rsp_json.device_list[i].device_Type
                });

                <%
                if(service_page_info!=null && current_topic_page_info!=null && current_sub_service_page_info!=null){
                    switch(current_sub_service_page_info.bind_unbind_tile_background_type){
                        case "css": %>
                            show_accessories_html.push("<div id=\"accessories_ctrl_tile_"+ i +"\" data-role=\"hint\" onclick=\"Show_BindUnbind_OP_Page('" + i + "')\" class=\"tile-medium <%=current_sub_service_page_info.bind_unbind_tile_background_css%> fg-white\"\
                                data-hint-position=\"top\" data-hint-text=\"" + rsp_json.device_list[i].device_Name + "\">");
                            <% switch(current_sub_service_page_info.bind_unbind_tile_icon_type){
                                case "css": %>
                                show_accessories_html.push("<span class=\"<%=current_sub_service_page_info.bind_unbind_tile_icon_css%> mif-4x icon\"></span>");
                                show_accessories_html.push("<span class=\"branding-bar device_name\" style=\"font-size: 20px\">" + rsp_json.device_list[i].device_Name + "</span>");
                                    <% break;
                                case "URL": %>
                                show_accessories_html.push("<img class=\"icon\" style=\"display: inline;\" src=\"<%=current_sub_service_page_info.bind_unbind_tile_icon_URL%>\"/>");
                                show_accessories_html.push("<span class=\"branding-bar device_name\" style=\"font-size: 20px\">" + rsp_json.device_list[i].device_Name + "</span>");
                                    <% break;
                            } %>
                            show_accessories_html.push("</div>");
                            <% break;
                        case "URL": %>
                            show_accessories_html.push("<div id=\"accessories_ctrl_tile_"+ i +"\" data-role=\"hint\" class=\"tile-medium fg-white\" data-hint-position=\"top\" data-hint-text=\"" + rsp_json.device_list[i].device_Name + "\"\
                                data-cover=\"<%=current_sub_service_page_info.bind_unbind_tile_background_URL%>\" onclick=\"Show_BindUnbind_OP_Page('" + i + "')\">");
                            show_accessories_html.push("<span class=\"branding-bar\">" + rsp_json.device_list[i].device_Name + "</span>");
                            show_accessories_html.push("</div>");
                            <% break;
                    }
                }
                %>
            }

            if(!$('#bind_unbind_display_area').hasClass('gridly'))
            {
                $('#bind_unbind_display_area').addClass('gridly');
            }

            $('#bind_unbind_display_area').html('');
            $('#bind_unbind_display_area').html(show_accessories_html.join(''));
            
            $('.gridly').trigger("change");
            $('.gridly').gridly('draggable', 'off');
            $('.gridly').gridly({'responsive': true});
        });
    }
